model FusionApk {
  id         String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  apk_name   String   @unique
  version    String   @unique
  file_path  String
  size       Int?
  key        String?
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  @@map("fusion_apks")
  @@schema("public")
}
